<h2>Users</h2>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
   <tbody>
    <% @users.each do |user| %>
      <tr>
      <td>ユーザの写真</td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>

</table>
